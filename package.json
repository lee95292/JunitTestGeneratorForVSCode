{
	"name": "testfile-generator-for-junit",
	"displayName": "testfile generator for junit",
	"description": "it provide context menu for generating junit test file",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		
		"menus":{
			"explorer/context": [
				{
					"command":"extension.generateTest",
					"when":"resourceExtname==.js"
				}
			],
			"editor/context": [
				{
					"command": "extension.helloWorld",
					"when": "resourceExtname==.js"
				}
			]
		},
		"commands": [
			{
				"command": "extension.generateTest",
				"title": "Generate Junit Testfile"
			},
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
